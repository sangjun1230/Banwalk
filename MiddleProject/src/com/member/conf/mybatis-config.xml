<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="my.properties" />
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      
      <dataSource type="POOLED">
        <property name="driver" value="${m_driver}"/>
        <property name="url" value="${m_url}"/>
        <property name="username" value="${m_username}"/>
        <property name="password" value="${m_password}"/>
      </dataSource> 
      
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/member/conf/memberMapper.xml"/>
   <mapper resource="com/member/conf/petMapper.xml"/>
  </mappers>

</configuration>
